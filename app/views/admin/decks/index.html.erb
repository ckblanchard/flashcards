<h1>Your decks from <%= current_admin.organization.name %></h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% current_admin.organization.decks.each do |deck| %>
  <tr>
    <td><%= link_to deck.name, admin_deck_path %></td>
    <td><%= deck.description %></td>
  </tr>
<% end %>
</table>

<br />

